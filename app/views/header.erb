<header>
	<img src='https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTjR_n6FVCx73pTvpf8fcBS_169EFOC4bvZSiFjRpbJz3zXo1MH'>
	<div id="logo">TwitFace</div>
	<a href="/peeps/new" class="button highlighted">Add link</a>
	<div id="user-peeps">
		<% if current_user %>
			<span id="welcome-message">
				Welcome to TwitFace, <%= current_user.username %>
				</span>

		    <form id="sign-out" method='post' action='/sessions'>
		      <input type='hidden' name='_method' value='delete'>
		      <input type='submit' value='Sign out'>
		    </form>
		  <% else %>
		  	<a href='/users/new' id='sign-up' class='button'>Sign up</a>
		  	<a href='/sessions/new' id='sign-in' class='button'>Sign in</a>
		  <% end %>	
		  
	</div>			
</header>